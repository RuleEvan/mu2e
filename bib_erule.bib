@article{LENZ1971513,
title = {Nuclear radii in the high-energy limit of elastic electron scattering},
journal = {Nuclear Physics A},
volume = {176},
number = {3},
pages = {513-525},
year = {1971},
issn = {0375-9474},
doi = {https://doi.org/10.1016/0375-9474(71)90933-X},
url = {https://www.sciencedirect.com/science/article/pii/037594747190933X},
author = {F. Lenz and R. Rosenfelder},
abstract = {From the formula for the energy-derivative of the phase shifts a systematic high-energy expansion of the phase shifts for the elastic scattering of electrons from nuclei is deduced. The coefficients of this expansion can be arranged in terms of inverse powers of the energy containing model-independent quantities of the nuclear charge distribution. It is shown that the mean logarithmic radius exp [〈1n r〉] and the inverse square radius 〈1/r2〉−12 determine the high-energy behaviour of the phase shifts for small values of the angular momenta. Applications to the elastic electron and positron scattering from nuclei and to dispersion effects are discussed.}
}

@article{KNOLL1974462,
title = {An analytic description of inelastic electron scattering on nuclei},
journal = {Nuclear Physics A},
volume = {223},
number = {3},
pages = {462-476},
year = {1974},
issn = {0375-9474},
doi = {https://doi.org/10.1016/0375-9474(74)90700-3},
url = {https://www.sciencedirect.com/science/article/pii/0375947474907003},
author = {Jörn Knoll},
abstract = {An expansion of the DWBA matrix element in terms of Born matrix elements and their derivatives with respect to momentum transfer q is deduced. A high energy approximation of electron waves allows an analytic determination of the expansion coefficients. A quantitative agreement with the exact amplitude is obtained at high momentum transfers. The behaviour of inelastic cross sections in the presence of strong Coulomb corrections is studied. Methods which separate charge and current contributions for heavy nuclei, are discussed.}
}

